#!/bin/bash
# Aá´œá´›Êœá´Ê€ Â» Uá´á´œT Ká´€Rá´€
# https://t.me/umutkaratools
version="v1.2"
scriptStatus="True"

True='âœ…'
False='â—'
info='ğŸ”¸'
w='\e[1;97m'
r='\e[1;31m'
g='\e[1;32m'
y='\e[1;33m'
scriptName=`basename "$0"`
scriptNameCount=`echo "$scriptName" |wc -m`

if [[ $1 == -e || $1 == --edit  ]];then
	if [[ -a $PREFIX/bin/vim ]];then
		vim $0
	fi
	exit
fi

if [[ $1 == -o || $1 == --open  ]];then
	if [[ -a $PREFIX/bin/termux-open ]];then
		termux-open $0
	fi
	exit
fi

if [[ ! -a $PREFIX/bin/git ]];then
	printf "\n\n\n${g}
	\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
	\r\tâ•‘ âœ… ${w}GÉªá´› Ká´œÊ€á´œÊŸá´œÊá´Ê€ ${g}â•‘
	\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
	pkg install git -y
fi

if [[ ! -a $PREFIX/bin/jq ]];then
	printf "\n\n\n${g}
	\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
	\r\tâ•‘ âœ… ${w}Jq Ká´œÊ€á´œÊŸá´œÊá´Ê€ ${g}â•‘
	\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
	pkg install jq -y
fi

if [[ ! -a $PREFIX/bin/curl ]];then
	printf "\n\n\n${g}
	\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
	\r\tâ•‘ âœ… ${w}Cá´œÊ€L Ká´œÊ€á´œÊŸá´œÊá´Ê€ ${g}â•‘
	\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
	pkg install curl -y
fi

function _autoUpdate () {

	# SCRÄ°PT UPDATE

	scriptVersion=$(curl -s https://raw.githubusercontent.com/Umutkara-tools/TermuxScripts/master/$scriptName |sed -n 4p |awk -F '"' '{print $2}')
	
	myScriptVersion=$(cat $0 |sed -n 4p |awk -F '"' '{print $2}')

	if [[ $scriptVersion != $myScriptVersion ]];then

		scriptStatus=$(curl -s https://raw.githubusercontent.com/Umutkara-tools/TermuxScripts/master/$scriptName |sed -n 5p |awk -F '"' '{print $2}')
		if [[ $scriptStatus == True ]];then
			clear
			printf "\n\n\n"
			read -e -p $'\t\e[1;97mYá´‡É´Éª Gá´œÉ´á´„á´‡ÊŸÊŸá´‡á´á´‡ Bá´œÊŸá´œÉ´á´…á´œ Sá´„Ê€Éªá´˜á´› Gá´œÉ´á´„á´‡ÊŸÊŸá´‡É´êœ±ÉªÉ´ MÉª? \e[33m[Ê/N] \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' update
			if [[ $update == y || $update == Y ]];then
				curl -s https://raw.githubusercontent.com/Umutkara-tools/TermuxScripts/master/$scriptName > $0
				printf "${g}
				\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
				\r\tâ•‘ âœ… ${w}ï¼³ï½ƒï½’ï½‰ï½ï½” ï¼§ï½•ï½ï½ƒï½…ï½Œï½Œï½…ï½ï½„ï½‰ ${g}â•‘
				\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
				exit
			fi
		fi
	fi
}

_autoUpdate

git config --global user.email "$(whoami)@localhost"
git config --global user.name "$(whoami)"


userName="No Username"
repoName="No Repo"
repoStatusIcon="â—"
userNameStatusIcon="â—"

if [[ -a .git ]];then
	repoControl=$(cat .git/config |grep -o "github.com" |wc -l)
fi

if [[ $repoControl == 1 ]];then
	userName=$(cat .git/config |grep url |awk -F'/' '{print $4}')
	repoName=$(cat .git/config |grep url |awk -F '.git' '{print $2}' | awk -F "/" '{print $3}')
	repoNameCount=$(echo "$repoName" |wc -m)
	userNameCount=$(echo "$userName" |wc -m)
	repoStatus="True"
	userNameStatus="True"
	sessionControl="True"
	repoStatusIcon="âœ…"
	if [[ -s  $HOME/.git-credentials ]];then
		userNameToken=$(cat $HOME/.git-credentials |awk -F ":" '{print $2}' |tr -d '/')
		if [[ $userName == $userNameToken ]];then
			sessionControl="False"
			userNameStatusIcon="âœ…"
			repoStatusIcon="âœ…"
		fi

	fi
else
	userNameStatus="False"
	userNameStatusIcon="â—"

	if [[ -s  $HOME/.git-credentials ]];then
		userName=$(cat $HOME/.git-credentials |awk -F ":" '{print $2}' |tr -d '/')
		userNameStatusIcon="âœ…"
		userNameStatus="True"
		sessionControl="False"
	fi

	repoStatus="False"
	repoStatusIcon="â—"
	repoNameCount=$(echo "$repoName" |wc -m)
	userNameCount=$(echo "$userName" |wc -m)

fi

if [[ -s  $HOME/.github-sessions ]];then
	
	if [[ $sessionControl == True ]];then
		sessionCount=$(cat $HOME/.github-sessions |wc -l)

		for count in `seq 1 $sessionCount`
		do
			userNames=$(cat $HOME/.github-sessions |sed -n ${count}p |awk -F ":" '{print $2}' |tr -d '/')
			if [[ $userName == $userNames ]];then
				selected=$(cat $HOME/.github-sessions |sed -n ${count}p)
				echo "$selected" > $HOME/.git-credentials
				userName=$(cat $HOME/.git-credentials |awk -F ":" '{print $2}' |tr -d '/')
				userNameCount=$(echo "$userName" |wc -m)
				repoStatusIcon="âœ…"
				userNameStatusIcon="âœ…"
			fi
		done
	fi
fi

let userNameCount+=8
let repoNameCount+=$userNameCount
border="â•"
if [[ -a $HOME/.result ]];then
	rm $HOME/.result
fi
for count in `seq 0 $repoNameCount`
do
	printf "$border" >> $HOME/.result
done
border=$(cat $HOME/.result)

function _logout (){
	read -e -p $'\t\e[1;97m[ \e[32mÃ‡Ä±á´‹Ä±ÅŸ Yá´€á´˜Ä±ÊŸêœ±Ä±É´ MÄ±? \e[33m[Ê/N] \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' exitScript
	if [[ $exitScript == y || $exitScript == Y || $exitScript == "" ]];then
		printf "\n\n\n${g}
		\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
		\r\tâ•‘ â— ${w}CÉªá´‹Éªêœ± Yá´€á´˜ÉªÊŸá´…Éª ${g}â•‘
		\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
		exit
	else
		sleep 1
		$0
		exit
	fi
}

function _github-sessions () {

sessionCount=$(cat $HOME/.github-sessions |wc -l)

for count in `seq 1 $sessionCount`
do
	userName=$(cat $HOME/.github-sessions |sed -n ${count}p |awk -F ":" '{print $2}' |tr -d '/')
	echo -e "\r\t${r}[${w}$count${r}]${w} $userName\n\n"

done

if [[ $1 == show ]];then
	_logout
fi

while true
do
	read -e -p $'\t\e[1;97m[ \e[32mSá´‡á´„á´‡É´á´‡á´‹ GÉªÊ€ÉªÉ´Éªá´¢ \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' options
	if [[ $options =~ ^[0-9]+$ ]];then
		if [[ $options -lt $sessionCount || $options == $sessionCount ]];then
			selected=$(cat $HOME/.github-sessions |sed -n ${options}p)
			if [[ $1 == select ]];then
				echo "$selected" > $HOME/.git-credentials
				printf "${g}
				\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
				\r\tâ•‘ âœ… ${w}Há´‡êœ±á´€á´˜ Sá´‡á´„ÉªÊŸá´…Éª ${g}â•‘
				\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
				break
			fi

			if [[ $1 == remove ]];then
				cat $HOME/.github-sessions |sed ${options}d > $HOME/.result
				cat $HOME/.result > $HOME/.github-sessions
				printf "${g}
				\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
				\r\tâ•‘ âœ… ${w}Há´‡êœ±á´€á´˜ Ká´€ÊŸá´…ÉªÊ€ÉªÊŸá´…Éª ${g}â•‘
				\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
				break
			fi
		else
			printf "\n${r}
			\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
			\r\tâ•‘ â— ${w}Há´€á´›á´€ÊŸÉª êœ±á´‡á´„Éªá´ ${r}â•‘
			\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
		fi
	else
		printf "\n${r}
		\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
		\r\tâ•‘ â— ${w}Sá´€ÊÉª GÉªÊ€ÉªÉ´Éªá´¢ ${r}â•‘
		\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
	fi

done

}

function _repoCheck () {

	while true
	do
		read -e -p $'\t\e[1;97m[ \e[32mDá´‡á´˜á´ Aá´…Éª GÉªÊ€ÉªÉ´Éªá´¢ \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' repoName
		history -s $repoName

		repoCheck=$(curl -s "https://github.com/$userName/$repoName" |grep -o '<img alt="404' |sed -n 1p |wc -l)
		
		if [[ $repoCheck == 0 ]];then
			break
		else
			printf "${g}
			\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
			\r\tâ•‘ â— ${w}Yá´‡É´Éª Dá´‡á´˜á´ OÊŸá´œêœ±á´›á´œÊ€á´œÉ´ ${g}â•‘
			\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
			sleep 2
			am start -a android.intent.action.VIEW "https://github.com/new"
			printf "\n\n\n"
		fi
	
	done
}

function _repoShowOptions () {
	read -e -p $'\t\e[1;97m[ \e[32mGá´Ê€á´œÉ´á´›á´œÊŸá´‡á´á´‡á´‹ Iêœ±á´›á´‡Ê€ á´Éªêœ±ÉªÉ´Éªá´¢? \e[33m[Ê/N] \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' repoShow
	if [[ $repoShow == y || $repoShow == Y || $repoShow == "" ]];then
		am start -a android.intent.action.VIEW "https://github.com/$userName/$repoName"
	fi
	printf "\n\n\n${g}
	\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
	\r\tâ•‘ â— ${w}CÉªá´‹Éªêœ± Yá´€á´˜ÉªÊŸá´…Éª ${g}â•‘
	\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
	exit
}

function _tokenControl () {

	while true
	do
		if [[ $1 == control ]];then
			
			if [[ -s $HOME/.git-credentials ]];then
				break
			else
				if [[ -s $HOME/.github-sessions ]];then
					break
				fi
			fi

		fi
		
		read -e -p $'\t\e[1;97m[ \e[32mKá´œÊŸÊŸá´€É´Éªá´„Éª Aá´…Éª GÉªÊ€ÉªÉ´Éªá´¢ \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' githubUserName
		history -s $githubUserNami
		printf "\n\n\n"
		read -e -p $'\t\e[1;97m[ \e[32mGÉªá´›Êœá´œÊ™ Tá´á´‹á´‡É´ GÉªÊ€ÉªÉ´Éªá´¢ \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' githubToken
		history -s $githubToken

		tokenControl=$(echo "$githubToken" |grep -Po '(ghp_)(.*?)' |wc -l)

		if [[ $tokenControl == 1 ]];then
			echo "https://$githubUserName:$githubToken@github.com" > $HOME/.git-credentials
			echo "https://$githubUserName:$githubToken@github.com" >> $HOME/.github-sessions
			printf "\n\n${g}
			\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
			\r\tâ•‘ âœ… ${w}Tá´á´‹á´‡É´ Ká´€Êá´…á´‡á´…ÉªÊŸá´…Éª ${g}â•‘
			\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
			break
		else
			printf "\n\n${g}
			\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
			\r\tâ•‘ â— ${w}Há´€á´›á´€ÊŸÉª Tá´á´‹á´‡É´ ${g}â•‘
			\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
		fi
	done

}


clear
printf "\n\n\e[32m
\t\t\t\t\t   â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—

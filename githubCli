if [[ $options == 1 ]];then
	
	# REPO CREATE

	_tokenControl control

	if [[ $repoStatus == True ]];then
		read -e -p $'\t\e[1;97m[ \e[32mDÉªá´¢ÉªÉ´á´…á´‡á´‹Éª GÉªá´› Dá´êœ±Êá´€ÊŸá´€Ê€Éª Ká´€ÊŸá´…ÉªÊ€ÉªÊŸêœ±ÉªÉ´ MÉª? \e[33m[Ê/N] \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' repoRemove
		printf "\n\n\n"
		if [[ $repoRemove == y || $repoRemove == Y ]];then
			rm -rf .git
			printf "${g}
			\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—

#!/bin/bash
# Aá´œá´›Êœá´Ê€ Â» Uá´á´œT Ká´€Rá´€
# https://t.me/umutkaratools
version="v1.3"
scriptStatus="True"

w='\e[1;97m'
r='\e[1;31m'
g='\e[1;32m'
y='\e[1;33m'
scriptName=`basename "$0"`
scriptNameCount=`echo "$scriptName" |wc -m`

if [[ $1 == -e || $1 == --edit  ]];then
	if [[ -a $PREFIX/bin/vim ]];then
		vim $0
	fi
	exit
fi

if [[ $1 == -o || $1 == --open  ]];then
	if [[ -a $PREFIX/bin/termux-open ]];then
		termux-open $0
	fi
	exit
fi

if [[ ! -a $PREFIX/bin/git ]];then
	printf "\n\n\n${g}
	\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
	\r\tâ•‘ âœ… ${w}GÉªá´› Ká´œÊ€á´œÊŸá´œÊá´Ê€ ${g}â•‘
	\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
	pkg install git -y
fi

if [[ ! -a $PREFIX/bin/curl ]];then
	printf "\n\n\n${g}
	\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
	\r\tâ•‘ âœ… ${w}Cá´œÊ€L Ká´œÊ€á´œÊŸá´œÊá´Ê€ ${g}â•‘
	\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
	pkg install curl -y
fi

if [[ ! -a $PREFIX/bin/node ]];then
	printf "\n\n\n${g}
	\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
	\r\tâ•‘ âœ… ${w}Ná´á´…á´‡Jêœ± Ká´œÊ€á´œÊŸá´œÊá´Ê€ ${g}â•‘
	\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
	pkg install nodejs -y
fi


if [[ ! -a $PREFIX/bin/heroku ]];then
	printf "\n\n\n${g}
	\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
	\r\tâ•‘ âœ… ${w}Há´‡Ê€á´á´‹á´œ Ká´œÊ€á´œÊŸá´œÊá´Ê€ ${g}â•‘
	\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
	npm i -g heroku
fi

function _autoUpdate () {

	# SCRÄ°PT UPDATE

	scriptVersion=$(curl -s https://raw.githubusercontent.com/Umutkara-tools/TermuxScripts/master/$scriptName |sed -n 4p |awk -F '"' '{print $2}')
	
	myScriptVersion=$(cat $0 |sed -n 4p |awk -F '"' '{print $2}')

	if [[ $scriptVersion != $myScriptVersion ]];then

		scriptStatus=$(curl -s https://raw.githubusercontent.com/Umutkara-tools/TermuxScripts/master/$scriptName |sed -n 5p |awk -F '"' '{print $2}')
		if [[ $scriptStatus == True ]];then
			clear
			printf "\n\n\n"
			read -e -p $'\t\e[1;97mYá´‡É´Éª Gá´œÉ´á´„á´‡ÊŸÊŸá´‡á´á´‡ Bá´œÊŸá´œÉ´á´…á´œ Sá´„Ê€Éªá´˜á´› Gá´œÉ´á´„á´‡ÊŸÊŸá´‡É´êœ±ÉªÉ´ MÉª? \e[33m[Ê/N] \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' update
			if [[ $update == y || $update == Y ]];then
				curl -s https://raw.githubusercontent.com/Umutkara-tools/TermuxScripts/master/$scriptName > $0
				printf "${g}
				\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
				\r\tâ•‘ âœ… ${w}ï¼³ï½ƒï½’ï½‰ï½ï½” ï¼§ï½•ï½ï½ƒï½…ï½Œï½Œï½…ï½ï½„ï½‰ ${g}â•‘
				\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
				exit
			fi
		fi
	fi
}

_autoUpdate

close () {
printf "\n${y}
\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
\r\tâ•‘ ğŸ”¸ ${w}Lá´œá´›Ò“á´‡É´ Bá´‡á´‹ÊŸá´‡ÊÉªÉ´Éªá´¢... ${y}â•‘
\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n${w}"
}

if [[ -s $HOME/.netrc ]];then
	whoamiName=$(cat $HOME/.netrc |sed -n 2p |awk -F "login" '{print $2}' |xargs -L1)
	#whoamiName=$(heroku whoami |sed -n 1p)
	whoamiStatusIcon="âœ…"
	whoamiStatus="True"

	if [[ $whoamiName == false ]];then
		whoamiName=""
		rm $HOME/.netrc
	fi
fi

if [[ ! -n $whoamiName ]];then
	whoamiName="Not logged in"
	whoamiStatusIcon="â—"
	whoamiStatus="False"
fi

whoamiNameCount=$(echo "$whoamiName" |wc -m)

appName="No App"
appNameCount=$(echo "$appName" |wc -m)

if [[ -a .git ]];then
	herokuControl=$(cat .git/config |grep -o "git.heroku.com" |wc -l)
fi

if [[ $herokuControl == 1 ]];then
	appName=$(cat .git/config |grep url |awk -F '.git' '{print $2}' | awk -F "/" '{print $2}')
	appNameCount=$(echo "$appName" |wc -m)
	appStatusIcon="âœ…"
	appStatus="True"

	if [[ $whoamiStatus == True ]];then
		appControl=$(heroku apps |grep -o ^$appName\$)
		if [[ ! -n $appControl ]];then
			appStatusIcon="â—"
			appStatus="NoPermission"
		fi
	fi
else
	appStatus="False"
	appStatusIcon="â—"

fi

let whoamiNameCount+=8
let appNameCount+=$whoamiNameCount

border="â•"
if [[ -a $HOME/.result ]];then
	rm $HOME/.result
fi
for count in `seq 0 $appNameCount`
do
	printf "$border" >> $HOME/.result
done
border=$(cat $HOME/.result)

function _logout (){
	read -e -p $'\t\e[1;97m[ \e[32mÃ‡Ä±á´‹Ä±ÅŸ Yá´€á´˜Ä±ÊŸêœ±Ä±É´ MÄ±? \e[33m[Ê/N] \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' exitScript

	if [[ $exitScript == y || $exitScript == Y || $exitScript == "" ]];then
		printf "\n\n\n${g}
		\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
		\r\tâ•‘ â— ${w}CÉªá´‹Éªêœ± Yá´€á´˜ÉªÊŸá´…Éª ${g}â•‘
		\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
		exit
	else
		printf "\n\n\n"
		sleep 1
		$0
		exit
	fi
}

function _appCheck () {

	while true
	do
		advance="True"

		if [[ $2 == new ]];then
			printf "\n\n\n"
			read -e -p $'\t\e[1;97m[ \e[32mYá´‡É´Éª UÊÉ¢á´œÊŸá´€á´á´€ Aá´…Éª GÉªÊ€ÉªÉ´Éªá´¢ \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' appName
			history -s $appName
			newName="$appName"
		fi

		if [[ $2 == change ]];then
			read -e -p $'\t\e[1;97m[ \e[32mDá´‡É¢Éªêœ±á´›ÉªÊ€ÉªÊŸá´‡á´„á´‡á´‹ UÊÉ¢á´œÊŸá´€á´á´€ Aá´…ÉªÉ´Éª GÉªÊ€ÉªÉ´Éªá´¢ \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' appName
			history -s $appName
			printf "\n\n\n"
			appControl=$(heroku apps |grep -o ^$appName\$)
			if [[ ! -n $appControl ]];then
				heroku apps
				printf "\n\n\n${g}
				\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
				\r\tâ•‘ â— ${w}UÊÉ¢á´œÊŸá´€á´á´€ Bá´œÊŸá´œÉ´á´€á´á´€á´…Éª ${g}â•‘
				\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
				advance="False"
			else
				changeName="$appName"
				break
			fi
		fi

		if [[ $2 == remove ]];then
			read -e -p $'\t\e[1;97m[ \e[32mKá´€ÊŸá´…ÉªÊ€ÉªÊŸá´€á´„á´€á´‹ UÊÉ¢á´œÊŸá´€á´á´€ Aá´…ÉªÉ´Éª GÉªÊ€ÉªÉ´Éªá´¢ \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' appName
			history -s $appName
			printf "\n\n\n"
			appControl=$(heroku apps |grep -o ^$appName\$)
			if [[ ! -n $appControl ]];then
				heroku apps
				printf "\n\n\n${g}
				\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
				\r\tâ•‘ â— ${w}UÊÉ¢á´œÊŸá´€á´á´€ Bá´œÊŸá´œÉ´á´€á´á´€á´…Éª ${g}â•‘
				\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
				advance="False"
			else
				removeName="$appName"
				break
			fi

		fi

		if [[ $2 == "" ]];then
			read -e -p $'\t\e[1;97m[ \e[32mUÊÉ¢á´œÊŸá´€á´á´€ Aá´…Éª GÉªÊ€ÉªÉ´Éªá´¢ \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' appName
			history -s $appName
		fi

		_advance () {
			appCheck=$(curl -s https://$appName.herokuapp.com |grep -o "www.herokucdn.com/error-pages/no-such-app.html" |wc -l)
			#appCheck=$(curl -s https://$appName.herokuapp.com |wc -l)

			if [[ $1 == True ]];then
				if [[ $appCheck != 0 ]];then
					Break="True"
				else
					printf "\n\n${g}
					\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
					\r\tâ•‘ â— ${w}UÊÉ¢á´œÊŸá´€á´á´€ Ká´œÊŸÊŸá´€É´ÉªÊŸá´ÉªÊá´Ê€ ${g}â•‘
					\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
				fi
			else
				if [[ $appCheck != 0 ]];then
					Break="True"
				else
					printf "\n\n${g}
					\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
					\r\tâ•‘ â— ${w}UÊÉ¢á´œÊŸá´€á´á´€ Ká´œÊŸÊŸá´€É´ÉªÊŸÉªÊá´Ê€ ${g}â•‘
					\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
				fi

			fi
		}

		if [[ $advance == True ]];then
			_advance $1
		fi

		if [[ $Break == True ]];then
			break
		fi
	done
}

function _appShowOptions () {
	read -e -p $'\t\e[1;97m[ \e[32mGá´Ê€á´œÉ´á´›á´œÊŸá´‡á´á´‡á´‹ Iêœ±á´›á´‡Ê€ á´Éªêœ±ÉªÉ´Éªá´¢? \e[33m[Ê/N] \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' appShow
	if [[ $appShow == y || $appShow == Y || $appShow == "" ]];then
		am start -a android.intent.action.VIEW "https://$appName.herokuapp.com"
	fi
	printf "\n\n\n${g}
	\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
	\r\tâ•‘ â— ${w}CÉªá´‹Éªêœ± Yá´€á´˜ÉªÊŸá´…Éª ${g}â•‘
	\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
	exit
}

function _heroku-sessions () {

sessionCount=$(cat $HOME/.heroku-sessions |wc -l)

for count in `seq 1 $sessionCount`
do
	whoamiName=$(cat $HOME/.heroku-sessions |sed -n ${count}p |awk -F ":" '{print $1}')
	echo -e "\r\t${r}[${w}$count${r}]${w} $whoamiName\n\n"

done

if [[ $1 == show ]];then
	_logout
fi

while true
do
	read -e -p $'\t\e[1;97m[ \e[32mSá´‡á´„á´‡É´á´‡á´‹ GÉªÊ€ÉªÉ´Éªá´¢ \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' options
	if [[ $options =~ ^[0-9]+$ ]];then
		if [[ $options -lt $sessionCount || $options == $sessionCount ]];then
			whoamiName=$(cat $HOME/.heroku-sessions |sed -n ${options}p |awk -F ":" '{print $1}')
			
			herokuApiKey=$(cat $HOME/.heroku-sessions |sed -n ${options}p |awk -F ":" '{print $2}')
			
			if [[ $1 == select ]];then
				
				echo -e "machine api.heroku.com\n\tlogin $whoamiName\n\tpassword $herokuApiKey\nmachine git.heroku.com\n\tlogin $whoamiName\n\tpassword $herokuApiKey" > $HOME/.netrc

				printf "${g}
				\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
				\r\tâ•‘ âœ… ${w}Há´‡êœ±á´€á´˜ Sá´‡á´„ÉªÊŸá´…Éª ${g}â•‘
				\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
				break
			fi

			if [[ $1 == remove ]];then
				cat $HOME/.heroku-sessions |sed ${options}d > $HOME/.result
				cat $HOME/.result > $HOME/.heroku-sessions
				printf "${g}
				\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
				\r\tâ•‘ âœ… ${w}Há´‡êœ±á´€á´˜ Ká´€ÊŸá´…ÉªÊ€ÉªÊŸá´…Éª ${g}â•‘
				\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
				break
			fi
		else
			printf "\n${r}
			\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
			\r\tâ•‘ â— ${w}Há´€á´›á´€ÊŸÉª êœ±á´‡á´„Éªá´ ${r}â•‘
			\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
		fi
	else
		printf "\n${r}
		\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
		\r\tâ•‘ â— ${w}Sá´€ÊÉª GÉªÊ€ÉªÉ´Éªá´¢ ${r}â•‘
		\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
	fi

done

}

function _apiKeyControl () {

	# APÄ° KEY SAVE OR CONTROL

	while true
	do
		if [[ $1 == control ]];then
			
			if [[ -s $HOME/.netrc ]];then
				break
			else
				if [[ -s $HOME/.heroku-sessions ]];then
					break
				fi
			fi

		fi

		printf "\n\n\n"
		read -e -p $'\t\e[1;97m[ \e[32mAá´˜Éª AÉ´á´€Êœá´›á´€Ê€Éª GÉªÊ€ÉªÉ´Éªá´¢ \e[97m] \e[32m~ \e[31mÂ»Â»\e[97m ' herokuApiKey
		history -s $herokuApiKey

		if [[ -s $HOME/.netrc ]];then
			netrcStatus="True"
			cat $HOME/.netrc > $HOME/.netrcOld
		fi

		echo -e "machine api.heroku.com\n\tlogin false\n\tpassword $herokuApiKey\nmachine git.heroku.com\n\tlogin false\n\tpassword $herokuApiKey" > $HOME/.netrc
		whoamiName=$(heroku whoami |sed -n 1p)

		if [[ -n $whoamiName ]];then
			
			if [[ $netrcStatus == True ]];then
				rm $HOME/.netrcOld
			fi

			echo -e "machine api.heroku.com\n\tlogin $whoamiName\n\tpassword $herokuApiKey\nmachine git.heroku.com\n\tlogin $whoamiName\n\tpassword $herokuApiKey" > $HOME/.netrc

			echo "$whoamiName:$herokuApiKey" >> $HOME/.heroku-sessions

			printf "\n\n${g}
			\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
			\r\tâ•‘ âœ… ${w}Aá´˜Éª Ká´€Êá´…á´‡á´…ÉªÊŸá´…Éª ${g}â•‘
			\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"

			break
		else

			if [[ $netrcStatus == True ]];then
				cat $HOME/.netrcOld > $HOME/.netrc
				rm $HOME/.netrcOld
			fi

			printf "\n\n${g}
			\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
			\r\tâ•‘ â— ${w}Há´€á´›á´€ÊŸÉª Aá´˜Éª ${g}â•‘
			\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
			am start -a android.intent.action.VIEW "https://dashboard.heroku.com/account"
		fi
	done


}

function _permissionControl () {

	if [[ $whoamiStatus == False ]];then
		printf "\n\n\n${g}
		\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
		\r\tâ•‘ â— ${w}Aá´˜Éª AÉ´á´€Êœá´›á´€Ê€Éª Bá´œÊŸá´œÉ´á´€á´á´€á´…Éª ${g}â•‘
		\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
		_apiKeyControl save
		printf "\n\n\n"
		_logout
	fi

	if [[ $1 == appStatus && $appStatus == NoPermission ]];then
		printf "\n\n\n${g}
		\r\tâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
		\r\tâ•‘ â— ${w}Bá´œ UÊÉ¢á´œÊŸá´€á´á´€á´…á´€ Iêœ±ÊŸá´‡á´ Yá´€á´˜á´á´€á´‹ Iá´„ÉªÉ´ Iá´¢É´ÉªÉ´Éªá´¢ Yá´á´‹ ${g}â•‘
		\r\tâ•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\n\n"
		_logout
	fi

}


clear
printf "\n\n\e[32m
\t\t\t\t\t   â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
\t\t\t\t\t   â•‘ ${r}<${w} Uá´á´œT Ká´€Rá´€ ${r}/> ${g}â•‘

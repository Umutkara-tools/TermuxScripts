		bul --swnop
	fi
	
	git init
	git add .
	git commit -am "new update"
	heroku apps:create $appName
	git push heroku master

	appCheck=$(curl -s https://$appName.herokuapp.com |grep -o "www.herokucdn.com/error-pages/no-such-app.html" |wc -l)
		
